  Owner                    = {paula123},
  Pmid                     = {24481239},
  Timestamp                = {2016.08.10},
  Url                      = {http://dx.doi.org/10.1039/c3nr05682a}
}

@Article{Gubicza2015,
  Title                    = {Resistive switching in metallic Ag2S memristors due to a local overheating induced phase transition.},
  Author                   = {Gubicza, Agnes and Csontos, Mikl{\'{o}}s and Halbritter, Andr{\'{a}}s and Mih{\'{a}}ly, Gy{\"{o}}rgy},
