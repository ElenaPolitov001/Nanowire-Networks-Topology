}

@Article{Ryu2010,
  Title                    = {Logic circuits based on individual semiconducting and metallic carbon-nanotube devices.},
  Author                   = {Ryu, Hyeyeon and K{\"{a}}lblein, Daniel and Weitz, R Thomas and Ante, Frederik and Zschieschang, Ute and Kern, Klaus and Schmidt, Oliver G. and Klauk, Hagen},
