  Url                      = {http://dx.doi.org/10.1039/c5nr02536b}
}

@Article{Gubicza2015a,
  Title                    = {Non-exponential resistive switching in Ag2S memristors: a key to nanometer-scale non-volatile memory devices.},
  Author                   = {Gubicza, Agnes and Csontos, Mikl{\'{o}}s and Halbritter, Andr{\'{a}}s and Mih{\'{a}}ly, Gy{\"{o}}rgy},
  Journal                  = {Nanoscale},
